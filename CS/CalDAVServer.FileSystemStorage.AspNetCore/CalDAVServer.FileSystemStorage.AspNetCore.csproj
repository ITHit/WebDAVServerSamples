			<CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
		</Content>
	</ItemGroup>
	<ItemGroup>
		<PackageReference Include="ITHit.Server" Version="13.2.12442-Beta" />
		<PackageReference Include="ITHit.Server.Core" Version="13.2.12442-Beta" />
		<PackageReference Include="ITHit.WebDAV.Server" Version="13.2.12442-Beta" />
		<PackageReference Include="ITHit.GSuite.Server" Version="13.2.12442-Beta" />
		<PackageReference Include="System.Data.OleDb" Version="6.0.0" />
	</ItemGroup>
	<ItemGroup>
		<PackageReference Include="ITHit.Collab" Version="1.0.0.759" />
	</ItemGroup>
</Project>