{
  "extends": "eslint:recommended",
  "ecmaFeatures": {
    "modules": true,
    "spread": true,
    "restParams": true
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "no-unused-vars": 0,
    "no-useless-escape": 0,
    "no-prototype-builtins": 0,
    "no-undef": 2
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2020
  }
}
